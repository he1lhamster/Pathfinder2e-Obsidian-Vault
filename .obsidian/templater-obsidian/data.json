{
  "command_timeout": 5,
  "templates_folder": "zz_templates",
  "templates_pairs": [
    [
      "myscr",
      "python zz_scripts/my_script.py <% tp.frontmatter.name %>"
    ]
  ],
  "trigger_on_file_creation": true,
  "auto_jump_to_cursor": true,
  "enable_system_commands": true,
  "shell_path": "",
  "user_scripts_folder": "zz_scripts",
  "enable_folder_templates": true,
  "folder_templates": [
    {
      "folder": "Curse Crimson Throne/PCs",
      "template": "zz_templates/PC template.md"
    },
    {
      "folder": "DATABASE/classes",
      "template": "zz_templates/class_template.md"
    }
  ],
  "syntax_highlighting": true,
  "enabled_templates_hotkeys": [
    "zz_templates/spell_template.md"
  ],
  "startup_templates": [
    ""
  ],
  "enable_ribbon_icon": true
}